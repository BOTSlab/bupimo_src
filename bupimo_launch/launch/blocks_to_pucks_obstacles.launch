<launch>
    <!-- Fake TF for testing -->
    <node pkg="tf" type="static_transform_publisher"
        name="static_transform_publisher" 
        args="0 0 0 0 0 0 map base_link 100"/>

    <param name = "number_puck_types" value="2"/>

    <param name = "obstacles_signature" value="3"/>

    <param name = "duplicate_puck_threshold" value="0.02"/>

    <node pkg="blocks_to_pucks_obstacles" type="blocks_to_pucks_obstacles.py" name="blocks_to_pucks_obstacles" output="screen"/>

<!--    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find blocks_to_pucks)/rviz.rviz"/> -->
</launch>
